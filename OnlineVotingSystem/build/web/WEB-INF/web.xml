    <servlet>
        <servlet-name>ManageNews</servlet-name>
        <servlet-class>ManageNews</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadJob</servlet-name>
        <servlet-class>UploadJob</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JobCandidates</servlet-name>
        <servlet-class>JobCandidates</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendMail</servlet-name>
        <servlet-class>SendMail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveVotes</servlet-name>
        <url-pattern>/SaveVotes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registraion</servlet-name>
        <url-pattern>/Registraion</url-pattern>
    </servlet-mapping>
