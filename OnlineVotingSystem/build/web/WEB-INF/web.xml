        <servlet-name>UploadJob</servlet-name>
        <url-pattern>/UploadJob</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JobCandidates</servlet-name>
        <url-pattern>/JobCandidates</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendMail</servlet-name>
        <url-pattern>/SendMail</url-pattern>
    </servlet-mapping>
